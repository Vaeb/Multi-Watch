{
  "name": "multi-watch",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "watch": "nodemon --exec \"echo 1\" --verbose --watch 'src/**/*' -e tsx,ts",
    "typecheck": "tsc --noEmit",
    "hook": "bun src/webhooks.js"
  },
  "dependencies": {
    "@types/video.js": "^7.3.58",
    "@virtuoso.dev/message-list": "1.9.9",
    "amazon-ivs-player": "^1.41.0",
    "clsx": "^2.1.1",
    "express": "^4.21.2",
    "geist": "^1.3.1",
    "next": "^15.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-twitch-embed": "^3.0.2",
    "server-only": "^0.0.1",
    "tree-kill": "^1.2.2",
    "video.js": "^8.23.3",
    "videojs-hls-quality-selector": "^2.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.12",
    "@types/express": "^5.0.0",
    "@types/node": "^20.17.19",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "bun-types": "^1.2.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.7",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "watch": "^1.0.2"
  },
  "packageManager": "npm@10.9.0"
}